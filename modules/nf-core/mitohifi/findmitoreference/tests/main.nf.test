
nextflow_process {

    name "Test Process MITOHIFI_FINDMITOREFERENCE"
    script "../main.nf"
    process "MITOHIFI_FINDMITOREFERENCE"

    tag "modules"
    tag "modules_nfcore"
    tag "mitohifi"
    tag "mitohifi/findmitoreference"

    test("test-mitohifi-findmitoreference") {

        when {
            process {
                """
                input[0] = [
				    [ id:'test' ],       // meta map
				    "Phalera flavescens" // species
				]

                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }

    test("test-mitohifi-findmitoreference-stub") {
        options '-stub'
        when {
            process {
                """
                input[0] = [
				    [ id:'test' ],       // meta map
				    "Phalera flavescens" // species
				]

                """
            }
        }

        then {
            assertAll(
                { assert process.success },
                { assert snapshot(process.out).match() }
            )
        }
    }

}
